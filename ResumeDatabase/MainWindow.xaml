                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="japanese">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox Tag="japanese" Text="{Binding Path=optionProp.japanese}" IsEnabled="{Binding isEnabled}" Margin="0">
                                                    </TextBox>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>

                                        </GridViewColumn>
                                        <GridViewColumn Header="intentionSalary" >
