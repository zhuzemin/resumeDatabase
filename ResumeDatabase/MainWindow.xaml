                                        <GridViewColumn Header="name">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox Tag="name" Text="{Binding necessaryProp.name}" IsEnabled="{Binding isEnabled}" Margin="0">
